single_alias[ordered_effect_score_operation] = {
    ## cardinality = 0..1
    limit = {
        alias_name[trigger] = alias_match_left[trigger]
    }
    ## cardinality = 0..1
    # TODO: Work out what exactly this is restricted to
    order_by = alias_name[int_value]
    ## cardinality = 0..1
    max = alias_name[int_value]
    ## cardinality = 0..1
    max = alias_name[int_value]
    ## cardinality = 0..1
    position = int
    ## cardinality = 0..1
    check_range_bounds = no
    ## cardinality = 0..inf
    alias_name[arithmetic_operation] = alias_match_left[arithmetic_operation]
    ## cardinality = 0..inf
    alias_name[score_operation] = alias_match_left[score_operation]
}

