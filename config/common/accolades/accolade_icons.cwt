types = {
    type[accolade_icon] = {
        path = "game/common/accolade_icons"
    }
}

accolade_icon = {

    ### The .dds file used for the icon.
    texture = filepath[game/gfx/interface/icons/knight_badge/icons]

    ### Determines if an icon can be used for the given accolade type
    potential = single_alias_right[trigger_clause]

}