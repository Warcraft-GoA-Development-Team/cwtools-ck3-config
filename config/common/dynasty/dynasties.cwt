types = {
	type[dynasty] = {
		path = "game/common/dynasties"
        localisation = {
            ## required
            ## primary
			Name = name
		}
	}
	type[house] = {
		path = "game/common/dynasty_houses"
        localisation = {
            ## required
            ## primary
			Name = name
		}
	}
}

dynasty = {
    ## cardinality = 0..1
    ### Prefix of the dynasty name.
	prefix = localisation
    ### Name of the dynasty.
	name = localisation
    ## cardinality = 0..1
    ### Optional motto of the dynasty. Will get a random one if not set.
	motto = localisation

	## cardinality = 0..1
	### Religion group dynasty is forced to be under.
    forced_coa_religiongroup = value[piety_icon_group]
    ### Which culture the dynasty belongs to.
	culture = localisation
}

house = {
    ## cardinality = 0..1
    ### Prefix of the house name.
	prefix = localisation
    ### Name of the house.
	name = localisation
    ### Which dynasty the house belongs to.
	dynasty = <dynasty>
}